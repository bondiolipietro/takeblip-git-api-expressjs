{
  "env": {
    "browser": true,
    "es2021": true
  },
  "plugins": ["prettier"],
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "class-methods-use-this": 0,
    "no-unused-vars": 1,
    "no-plusplus": 0,
    "no-param-reassign": 1,
    "consistent-return": 1
  }
}
